<table class="table table-hover">
    <thead>
        <tr>
          <th scope="col">STT</th>
          <th scope="col">username</th>
          <th scope="col">password</th>
          <th scope="col">fullname</th>
          <th scope="col">address</address></th>
          <th scope="col">sex</th>
          <th scope="col">email</th>
          <th scope="col">groupid</th>
          
        </tr>
      </thead>
      <tbody>
        <% for(let i=0;i<data.rows.length;i++){%>
          <tr>
            <th scope="row"><%=1+1 %></th>
            <td><%= data.rows[i].username %></td>
            <td><%= data.rows[i].password %></td>
            <td><%= data.rows[i].fullname %></td>
            <td>N<%= data.rows[i].address %></td>
            <td><%= data.rows[i].sex %></td>
            <td><%= data.rows[i].email %></td>
            <td><%= data.rows[i].groupid %></td>
            <td>
              <a href="/detailUser/<%= data.rows[i].username %>"><span class="setting"><i class="fa fa-eye" aria-hidden="true"></i></span>
              <a href="/edituser/<%= data.rows[i].username %>"><span class="setting"><i class="fa fa-pencil-alt" aria-hidden="true"></i></span>
              <form action="/delete-user" method="post" class="del">
                <input type="hidden" value="<%= data.rows[i].username %>" name="username"/>
                <label for="data.rows[i].username"><span class="setting"><i class="fa fa-trash" aria-hidden="true"></i></span></label>
                <input id="data.rows[i].username" type="submit" value="xoa" style="display: none;"/>
              </form>
              
            </td>
          </tr>
        <%}%>
        
      </tbody>
</table>